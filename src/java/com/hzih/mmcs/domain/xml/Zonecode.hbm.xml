<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class table="district" name="com.hzih.mmcs.domain.ZoneCode">
        <id name="zonecode" type="java.lang.String" >
            <column name="district_id" />
        </id>
        <property name="zonename" column="district_name" access="field" type="java.lang.String"/>
    </class>
</hibernate-mapping>