<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="sysdeviceinf" name="com.hzih.mmcs.domain.SysDeviceInf">
    <id access="field" name="id" type="java.lang.Integer" column="id">
      <generator class="identity"/>
    </id>
    <property name="idsystem" column="idsystem" access="field" type="java.lang.String"/>
    <property name="iddevice" column="iddevice" access="field" type="java.lang.Integer"/>
    <property name="devicedesc" column="devicedesc" access="field" type="java.lang.String"/>
    <property name="devicetypecode" column="devicetypecode" access="field" type="java.lang.String"/>
    <property name="deviceIP" column="deviceIP" access="field" type="java.lang.String"/>
    <property name="brandtyoe" column="brandtyoe" access="field" type="java.lang.String"/>
    <property name="collecttime" column="collecttime" access="field" type="java.sql.Timestamp"/>
  </class>
</hibernate-mapping>
