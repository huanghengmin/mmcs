<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class table="account_role" name="com.hzih.mmcs.domain.AccountRole">
        <id access="field" name="accountId" type="long" column="account_id">
            <generator class="increment"/>
        </id>
        <property name="roleId" column="role_id" access="field" type="long"/>
    </class>
</hibernate-mapping>