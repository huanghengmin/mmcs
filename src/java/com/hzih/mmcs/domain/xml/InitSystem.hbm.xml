<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="init_system" name="com.hzih.mmcs.domain.InitSystem">
    <id access="field" name="id" type="long" column="id" length="10">
      <generator class="increment"/>
    </id>
    <property name="rootCode" length="6" column="root_code" access="field" type="string"/>
  </class>
</hibernate-mapping>
